<link href="/css/Derek.css" rel="stylesheet" type="text/css">

<div class="content">
  <div class="squeeze">
    <div class="searchresults">
      {{#each feed}}
      <img src="{{this.images.standard_resolution.url}}" alt="" />
      <div>
      <h3>{{this.likes.count}} likes</h3>
      <h3>{{this.comments.count}} comments</h3><br>
        {{#if this.tags}}
        <h3>tags</h3>
        <ul>
          {{#each this.tags}}
          <li><a href="/user/search/{{this}}">{{this}}</a>,</li>
          {{/each}}
        </ul>
        {{/if}}
        <a id="likebutton" href="#"></a>
      </div>
      {{/each}}
    </div>
  </div>
</div>
